class fct{
    int fact(int n){
        if(n == 0){
            return 1;
        }
        else{
            return n*fact(n-1);
        }
    }
    int fibo (int n){
        int x;
        int y;
        if(n == 0){
            return 0;
        }
        else if(n == 1){
            return 1;
        }
        else{
             x = fibo(n-2);
            y = fibo(n-1);
            return x + y;
        }
}
}
{
fct f = new fct();
int n = readInt();
int x = f.fibo(n);
println(x);
}
